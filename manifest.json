{
    "manifest_version": 3,
    "name": "bot4shuffle",
    "description": "a javascript casino bot",
    "version": "1.0.0.0",
    "icons": {
        "128": "shuffle.png"
    },
    "options_page": "",
    "action": {
        "default_title": "bot4shuffle"
    },
    "permissions": [],
    "host_permissions": [
        "*://*/*"
    ],
    "background": {
    },
    "content_scripts": [{
        "js": ["index.js", "js/codemirror.min.js", "js/canvasjs.min.js", "js/javascript.min.js"],
        "matches": ["https://shuffle.com/*", "https://shuffle.bet/*", "https://shuffle.game/*"],
		"run_at": "document_end"
    }],
	"web_accessible_resources": [
    {
      "resources": [ "js/codemirror.min.js", "js/canvasjs.min.js", "js/javascript.min.js" ],
      "matches": [ "https://*/*" ]
    }
	],
	"content_security_policy": {
		"extension_pages": "script-src 'self'; object-src 'self';"
	}
}